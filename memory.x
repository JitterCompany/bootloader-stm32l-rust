MEMORY
{
  FLASH         : ORIGIN = 0x08000000, LENGTH = 32K
  FLASH_USER    : ORIGIN = 0x08000000 + LENGTH(FLASH), LENGTH = 192K - LENGTH(FLASH)
  RAM           : ORIGIN = 0x20000000, LENGTH = 20K
}

__FLASH_START = ORIGIN(FLASH);
__FLASH_USER_START = ORIGIN(FLASH_USER);
__FLASH_USER_LENGTH = LENGTH(FLASH_USER);
